.user-info {
    box-sizing: border-box;
    width: 750rpx;
    height: 280rpx;
    padding: 30rpx 0;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-around;
    color: #666;
}



.user-nickname {
    color: #111;
    font-size: 40rpx;
}

.user-sex-male, .user-sex-female {
    display: inline-block;
    width: 40rpx;
    height: 40rpx;
    margin-bottom: -5rpx;
    background-position: center;
    background-repeat: no-repeat;
    background-size: cover;
}

.user-sex-male {
    background-image: url(data:image/png;base64,);
}

.user-sex-female {
    background-image: url(data:image/png;base64,);
}

.user-avatar {
    width: 200rpx;
    height: 200rpx;
    border-radius: 200rpx;
    box-shadow: 0 0 300rpx 100rpx rgba(255, 255, 255, .2), 0 1px 0 rgba(255, 255, 255, 0.4);
}

.user-location {
    font-size: 24rpx;
    color: #111;
}
.empty{
  height: 30rpx;
  background: #eee;
}
.grids{
  padding-top: 10rpx;
}
.grids .grids_item{
  border-bottom: 1px solid #eee;
  height: 80rpx;
  line-height: 80rpx;
  padding-left: 20rpx;
  background: url(data:image/png;base64,) no-repeat right center;
  background-size: 32rpx 32rpx;
}

